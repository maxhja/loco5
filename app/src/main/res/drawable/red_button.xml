<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#ed4c00" />
      <stroke
          android:width="1dp"
          android:color="#ed4c00" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="30dp"
          android:top="10dp"
          android:right="30dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
       <solid android:color="#f06524" />
      <stroke
          android:width="1dp"
          android:color="#f06524" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="30dp"
          android:top="10dp"
          android:right="30dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>